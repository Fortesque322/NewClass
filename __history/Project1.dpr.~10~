program Project1;

{$APPTYPE CONSOLE}

{$R *.res}

uses
  System.SysUtils,
  Unit1 in 'Unit1.pas';

var
  x: string;
  Auto : TCar;

begin
  try
    Auto := TCar.Create(255, rewr, FiveDoors, 'Honda', 'Red', 'Japan', false);
    Readln(x);
    Writeln('Марка: ' +  Auto.NameCar);
    Readln;
  except
    on E: Exception do
      Writeln(E.ClassName, ': ', E.Message);
  end;
end.
